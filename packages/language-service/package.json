{
	"name": "@webgal/language-service",
	"version": "0.0.2-alpha.1",
	"type": "module",
	"main": "build/index.cjs",
	"module": "build/index.mjs",
	"types": "build/index.d.mts",
	"license": "MPL-2.0",
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"files": [
		"build/**/*"
	],
	"exports": {
		".": {
			"types": "./build/index.d.mts",
			"browser": "./build/index.mjs",
			"import": "./build/index.mjs",
			"require": "./build/index.cjs"
		},
		"./node": {
			"types": "./build/node.d.mts",
			"import": "./build/node.mjs",
			"require": "./build/node.cjs"
		},
		"./monaco": {
			"types": "./build/monaco.d.mts",
			"import": "./build/monaco.mjs",
			"require": "./build/monaco.cjs"
		},
		"./monaco-init": {
			"types": "./build/monaco-init.d.mts",
			"import": "./build/monaco-init.mjs",
			"require": "./build/monaco-init.cjs"
		},
		"./syntaxes": {
			"types": "./build/syntaxes.d.mts",
			"import": "./build/syntaxes.mjs",
			"require": "./build/syntaxes.cjs"
		},
		"./themes": {
			"types": "./build/themes.d.mts",
			"import": "./build/themes.mjs",
			"require": "./build/themes.cjs"
		}
	},
	"scripts": {
		"build": "tsdown",
		"format": " prettier --write \"src/**/*.ts\"",
		"lint": "eslint src --ext ts --fix",
		"typecheck": "tsc --noEmit",
		"watch": "tsdown --watch"
	},
	"devDependencies": {
		"@types/node": "^18.0.0",
		"monaco-editor": "^0.55.1",
		"tsdown": "^0.20.3"
	},
	"dependencies": {
		"@codingame/monaco-vscode-api": "^26.1.1",
		"@codingame/monaco-vscode-languages-service-override": "^26.1.1",
		"@codingame/monaco-vscode-textmate-service-override": "^26.1.1",
		"@codingame/monaco-vscode-theme-service-override": "^26.1.1",
		"@volar/language-core": "^2.4.28",
		"@volar/language-service": "^2.4.28",
		"vscode": "npm:@codingame/monaco-vscode-extension-api@26.1.1",
		"vscode-uri": "^3.1.0"
	}
}
